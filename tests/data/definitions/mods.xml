<?xml version="1.0" encoding="UTF-8"?>
<definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../XMLFormDefinition.xsd">
    <properties>
        <document>
            <root>
                <local_name>mods</local_name>
                <prefix>mods</prefix>
                <uri>http://www.loc.gov/mods/v3</uri>
            </root>
            <namespaces>
                <namespace prefix="mods">http://www.loc.gov/mods/v3</namespace>
                <namespace prefix="xlink">http://www.w3.org/1999/xlink</namespace>
                <namespace prefix="xsi">http://www.w3.org/2001/XMLSchema-instance</namespace>
                <namespace prefix="xmlns">http://www.w3.org/2000/xmlns</namespace>
            </namespaces>
            <schema>http://www.loc.gov/standards/mods/v3/mods-3-4.xsd</schema>
        </document>
    </properties>
    <form>
        <properties>
            <attributes>
                <enctype>multipart/form-data</enctype>
            </attributes>
        </properties>
        <children>
            <element name="indicator">
                <children>
                    <element name="ingest-file-location">
                        <properties>
                            <type>file</type>
                            <title>Upload Document</title>
                            <size>48</size>
                            <description>Select file to be added the the object.</description>
                        </properties>
                    </element>
                </children>
            </element>
            <!-- Co-Alliance form. -->
            <element name="title">
                <properties>
                    <type>textfield</type>
                    <title>Title</title>
                    <required>true</required>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;titleInfo&gt;&lt;title&gt;%value%&lt;/title&gt;&lt;/titleInfo&gt;</xml>
                        </create>
                        <read>
                            <path>/mods:mods/mods:titleInfo[not(@type)]/mods:title</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
            </element>
            <element name="alternative_title">
                <properties>
                    <type>textfield</type>
                    <title>Alternative Title</title>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;titleInfo type='alternative'&gt;&lt;title&gt;%value%&lt;/title&gt;&lt;/titleInfo&gt;</xml>
                        </create>
                        <read>
                            <path>/mods:mods/mods:titleInfo[@type='alternative']/mods:title</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
            </element>
            <element name="creator">
                <properties>
                    <type>tabs</type>
                    <title>Creator</title>
                    <collapsed>false</collapsed>
                    <collapsible>true</collapsible>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;name type='personal'&gt;&lt;role&gt;&lt;roleTerm authority="marcrelator" type="text"&gt;creator&lt;/roleTerm&gt;&lt;/role&gt;&lt;/name&gt;</xml>
                        </create>
                        <read>
                            <path>(/mods:mods/mods:name[@type='personal']/mods:role[mods:roleTerm='creator']/..)</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
                <children>
                    <element>
                        <properties>
                            <type>tabpanel</type>
                        </properties>
                        <children>
                            <element name="namePart">
                                <properties>
                                    <type>textfield</type>
                                    <title>Creator</title>
                                    <actions>
                                        <create>
                                            <path>self::node()</path>
                                            <context>parent</context>
                                            <element>namePart</element>
                                        </create>
                                        <read>
                                            <path>mods:namePart[not(@type)]</path>
                                            <context>parent</context>
                                        </read>
                                        <update>
                                            <path>self::node()</path>
                                            <context>self</context>
                                        </update>
                                    </actions>
                                </properties>
                            </element>
                        </children>
                    </element>
                </children>
            </element>
            <element name="contributor">
                <properties>
                    <type>tabs</type>
                    <title>Contributor</title>
                    <collapsed>false</collapsed>
                    <collapsible>true</collapsible>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;name type='personal'&gt;&lt;role&gt;&lt;roleTerm authority="marcrelator" type="text"&gt;contributor&lt;/roleTerm&gt;&lt;/role&gt;&lt;/name&gt;</xml>
                        </create>
                        <read>
                            <path>(/mods:mods/mods:name[@type='personal']/mods:role[mods:roleTerm='contributor']/..)</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
                <children>
                    <element>
                        <properties>
                            <type>tabpanel</type>
                        </properties>
                        <children>
                            <element>
                                <properties>
                                    <type>textfield</type>
                                    <title>Contributor</title>
                                    <actions>
                                        <create>
                                            <path>self::node()</path>
                                            <context>parent</context>
                                            <element>namePart</element>
                                        </create>
                                        <read>
                                            <path>mods:namePart[not(@type)]</path>
                                            <context>parent</context>
                                        </read>
                                        <update>
                                            <path>self::node()</path>
                                            <context>self</context>
                                        </update>
                                    </actions>
                                </properties>
                            </element>
                        </children>
                    </element>
                </children>
            </element>
            <element name="corporate_creator">
                <properties>
                    <type>tabs</type>
                    <title>Corporate Creator</title>
                    <collapsed>false</collapsed>
                    <collapsible>true</collapsible>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;name type='corporate'&gt;&lt;role&gt;&lt;roleTerm authority="marcrelator" type="text"&gt;creator&lt;/roleTerm&gt;&lt;/role&gt;&lt;/name&gt;</xml>
                        </create>
                        <read>
                            <path>(/mods:mods/mods:name[@type='corporate']/mods:role[mods:roleTerm='creator']/..)</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
                <children>
                    <element>
                        <properties>
                            <type>tabpanel</type>
                        </properties>
                        <children>
                            <element>
                                <properties>
                                    <type>textfield</type>
                                    <title>Creator</title>
                                    <actions>
                                        <create>
                                            <path>self::node()</path>
                                            <context>parent</context>
                                            <element>namePart</element>
                                        </create>
                                        <read>
                                            <path>mods:namePart[not(@type)]</path>
                                            <context>parent</context>
                                        </read>
                                        <update>
                                            <path>self::node()</path>
                                            <context>self</context>
                                        </update>
                                    </actions>
                                </properties>
                            </element>
                        </children>
                    </element>
                </children>
            </element>
            <element name="corporate_contributor">
                <properties>
                    <type>tabs</type>
                    <title>Corporate Contributor</title>
                    <collapsed>false</collapsed>
                    <collapsible>true</collapsible>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;name type='corporate'&gt;&lt;role&gt;&lt;roleTerm authority="marcrelator" type="text"&gt;contributor&lt;/roleTerm&gt;&lt;/role&gt;&lt;/name&gt;</xml>
                        </create>
                        <read>
                            <path>(/mods:mods/mods:name[@type='corporate']/mods:role[mods:roleTerm='contributor']/..)</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
                <children>
                    <element>
                        <properties>
                            <type>tabpanel</type>
                        </properties>
                        <children>
                            <element>
                                <properties>
                                    <type>textfield</type>
                                    <title>Contributor</title>
                                    <actions>
                                        <create>
                                            <path>self::node()</path>
                                            <context>parent</context>
                                            <element>namePart</element>
                                        </create>
                                        <read>
                                            <path>mods:namePart[not(@type)]</path>
                                            <context>parent</context>
                                        </read>
                                        <update>
                                            <path>self::node()</path>
                                            <context>self</context>
                                        </update>
                                    </actions>
                                </properties>
                            </element>
                        </children>
                    </element>
                </children>
            </element>
            <element name="type">
                <properties>
                    <type>select</type>
                    <title>Type of Resource</title>
                    <options>
                        <option key="NULL">Please Select One...</option>
                        <option key="text">text</option>
                        <option key="cartographic">cartographic</option>
                        <option key="sound recording">sound recording</option>
                        <option key="still image">still image</option>
                        <option key="moving image">moving image</option>
                        <option key="mixed material">mixed material</option>
                    </options>
                    <required>true</required>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                        </create>
                        <read>
                            <path>/mods:mods/mods:typeOfResource</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
            </element>
            <element name="origin_information">
                <properties>
                    <type>fieldset</type>
                    <title>Origin Information</title>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <element>originInfo</element>
                        </create>
                        <read>
                            <path>/mods:mods/mods:originInfo</path>
                            <context>document</context>
                        </read>
                    </actions>
                </properties>
                <children>
                    <element name="place">
                        <properties>
                            <type>textfield</type>
                            <title>Place of Publication</title>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                    <xml>&lt;place&gt;&lt;placeTerm type='text'&gt;%value%&lt;/placeTerm&gt;&lt;/place&gt;</xml>
                                </create>
                                <read>
                                    <path>mods:place/mods:placeTerm[@type='text']</path>
                                    <context>parent</context>
                                </read>
                            </actions>
                        </properties>
                    </element>
                    <element name="publisher">
                       <properties>
                           <type>textfield</type>
                           <title>Publisher</title>
                           <actions>
                               <create>
                                   <path>self::node()</path>
                                   <context>parent</context>
                                   <element>publisher</element>
                               </create>
                               <read>
                                   <path>mods:publisher</path>
                                   <context>parent</context>
                               </read>
                           </actions>
                       </properties>
                   </element>
                    <element name="issued">
                        <properties>
                            <type>textfield</type>
                            <title>Date Issued</title>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                </create>
                                <read>
                                    <path>mods:dateIssued</path>
                                    <context>parent</context>
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                    <element name="created">
                        <properties>
                            <type>textfield</type>
                            <title>Date Created</title>
                            <required>true</required>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                </create>
                                <read>
                                    <path>mods:dateCreated</path>
                                    <context>parent</context>
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                    <element name="copyright">
                        <properties>
                            <type>textfield</type>
                            <title>Copyright Date</title>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                </create>
                                <read>
                                    <path>mods:copyrightDate</path>
                                    <context>parent</context>
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                    <element name="other">
                        <properties>
                            <type>textfield</type>
                            <title>Other Date</title>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                    <element>dateOther</element>
                                </create>
                                <read>
                                    <path>mods:dateOther</path>
                                    <context>parent</context>                                    
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                </children>
            </element>
            <element name="genre">
                <properties>
                    <type>textfield</type>
                    <title>Genre</title>
                    <description>MARC Genre Term List (marcgt): http://www.loc.gov/standards/valuelist/marcgt.html</description>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;genre authority="marcgt"&gt;%value%&lt;/genre&gt;</xml>
                        </create>
                        <read>
                            <path>/mods:mods/mods:genre[@type='marcgt']</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
            </element>
            <element name="language">
                <properties>
                    <type>textfield</type>
                    <title>Language</title>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <xml>&lt;language&gt;&lt;languageTerm type="text"&gt;%value%&lt;/languageTerm&gt;&lt;/language&gt;</xml>
                        </create>
                        <read>
                            <path>/mods:mods/mods:language/mods:languageTerm</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
            </element>
            <element name="physical_description">
                <properties>
                    <type>fieldset</type>
                    <title>Physical Description</title>
                    <collapsed>false</collapsed>
                    <collapsible>true</collapsible>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                            <element>physicalDescription</element>
                        </create>
                        <read>
                            <path>/mods:mods/mods:physicalDescription</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
                <children>
                    <element name="extent">
                        <properties>
                            <type>textfield</type>
                            <title>Extent</title>
                            <description>Include units (e.g., pages)</description>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                </create>
                                <read>
                                    <path>mods:extent</path>
                                    <context>parent</context>                                    
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>                                    
                                </update>
                            </actions>
                        </properties>
                    </element>
                    <element name="form">
                        <properties>
                            <type>textfield</type>
                            <title>Form</title>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                </create>
                                <read>
                                    <path>mods:form</path>
                                    <context>parent</context>
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                    <element name="origin">
                        <properties>
                            <type>select</type>
                            <title>Digitial Origin</title>
                            <options>
                                <option key="NULL">Please Select One...</option>
                                <option key="born digital">born digital</option>
                                <option key="reformatted digital">reformatted digital</option>
                                <option key="digitized microfilm">digitized microfilm</option>
                                <option key="digitized other analog">digitized other analog</option>
                            </options>
                            <required>true</required>
                            <actions>
                                <create>
                                    <path>self::node()</path>
                                    <context>parent</context>
                                </create>
                                <read>
                                    <path>mods:digitalOrigin</path>
                                    <context>parent</context>
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                    <element name="physical_note">
                        <properties>
                            <type>tabs</type>
                            <title>Physical Note</title>
                        </properties>
                        <children>
                            <element>
                                <properties>
                                    <type>tabpanel</type>
                                </properties>
                                <children>
                                    <element>
                                        <properties>
                                            <type>textarea</type>
                                            <title>Note</title>
                                            <actions>
                                                <create>
                                                    <path>self::node()</path>
                                                    <context>parent</context>
                                                </create>
                                                <read>
                                                    <path>mods:note</path>
                                                    <context>parent</context>
                                                </read>
                                                <update>
                                                    <path>self::node()</path>
                                                    <context>self</context>
                                                </update>
                                            </actions>
                                        </properties>
                                    </element>
                                </children>
                            </element>
                        </children>
                    </element>
                </children>
            </element>
            <element name="description">
                <properties>
                    <type>textarea</type>
                    <title>Description</title>
                    <actions>
                        <create>
                            <path>/mods:mods</path>
                            <context>document</context>
                        </create>
                        <read>
                            <path>/mods:mods/mods:abstract</path>
                            <context>document</context>
                        </read>
                        <update>
                            <path>self::node()</path>
                            <context>self</context>
                        </update>
                    </actions>
                </properties>
            </element>
            <element name="target_audience">
                <properties>
                    <type>tags</type>
                    <title>Target Audience</title>
                </properties>
                <children>
                    <element>
                        <properties>
                            <type>tag</type>
                            <actions>
                                <create>
                                    <path>/mods:mods</path>
                                    <context>document</context>
                                    <element>targetAudience</element>
                                </create>
                                <read>
                                    <path>/mods:mods/mods:targetAudience</path>
                                    <context>document</context>
                                </read>
                                <update>
                                    <path>self::node()</path>
                                    <context>self</context>
                                </update>
                            </actions>
                        </properties>
                    </element>
                </children>
            </element>
            <element name="notes">
                <properties>
                    <type>tabs</type>
                    <title>Notes</title>
                </properties>
                <children>
                    <element>
                        <properties>
                            <type>tabpanel</type>
                            <actions>
                                <read>
                                    <path>/mods:mods/mods:note[not(@type)]</path>
                                    <context>document</context>
                                </read>
                            </actions>
                        </properties>
                        <children>
                            <element>
                                <properties>
                                    <type>textarea</type>
                                    <title>Note</title>
                                    <actions>
                                        <create>
                                            <path>/mods:mods</path>
                                            <context>document</context>
                                            <element>note</element>
                                        </create>
                                        <read>
                                            <path>self::node()</path>
                                            <context>parent</context>
                                        </read>
                                        <update>
                                            <path>self::node()</path>
                                            <context>self</context>
                                        </update>
                                    </actions>
                                </properties>
                            </element>
                        </children>
                    </element>
                </children>
            </element>
