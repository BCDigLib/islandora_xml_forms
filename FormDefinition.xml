<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://localhost/FormDefinition.xsd">
    <properties>
        <document_properties>
            <root>
                <local_name>mods</local_name>
                <prefix>mods</prefix>
                <uri>http://www.loc.gov/mods/v3</uri>
            </root>
            <namespaces>
                <namespace prefix="mods">http://www.loc.gov/mods/v3</namespace>
            </namespaces>
            <!-- <schema name="mods">http://www.loc.gov/standards/mods/v3/mods-3-4.xsd</schema> -->
        </document_properties>
    </properties>
    <elements>
        <element key="d">
            <type>tabs</type>
            <title>d</title>
            <tree>true</tree>
            <children>
                <element key="t">
                    <type>tabpanel</type>
                    <children>
                        <element key="e">
                            <type>textfield</type>
                            <default_value type="string">one</default_value>
                            <actions>
                                <read>
                                    <path context="parent">mods:title</path>
                                </read>
                            </actions>
                        </element>
                    </children>
                    <actions>
                        <read>
                            <path context="document">/mods:mods/mods:titleInfo</path>
                        </read>
                    </actions>
                </element>
            </children>
        </element>
        <element>
            <type>submit</type>
            <value>submit</value>
        </element>
    </elements>
</form>
